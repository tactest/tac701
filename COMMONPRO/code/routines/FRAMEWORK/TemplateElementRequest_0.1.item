package routines;

/**
 * Interface defining a Method for requesting an unset Template-Value.<p>
 */
public interface TemplateElementRequest {


  /**
   * Get the Object for an unset Template.
   * @param t the Template requesting the row.
   * @param data data required for all requests.
   * @param name the name of the unset template (in upper-case).
   * @param orgName the name of the unset template (as specified, not converted to upper-case).
   */
  public Object getTemplateValue( Template t, Object data, String name, String orgName );

}


